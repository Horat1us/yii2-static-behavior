{
  "name": "horat1us/yii2-static-behavior",
  "description": "Static Behavior for Yii2",
  "type": "library",
  "require": {
    "php": ">=7.2",
    "yiisoft/yii2": "^2.0.15"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "squizlabs/php_codesniffer": "^3.4"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Alexander Letnikow",
      "email": "reclamme@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Horat1us\\Yii\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Horat1us\\Yii\\Tests\\": "tests/"
    }
  },
  "config": {
    "process-timeout": 1800,
    "platform": {
      "php": "7.2"
    }
  },
  "scripts": {
    "lint": "./vendor/bin/phpcs --standard=PSR2 ./src ./tests",
    "phpcbf": "./vendor/bin/phpcbf --standard=PSR2 ./src ./tests",
    "test": "./vendor/bin/phpunit"
  },
  "keywords": [
    "yii2",
    "static",
    "behavior",
    "bootstrap"
  ]
}
